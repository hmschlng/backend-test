	<button id="testBtn">
		test1
	</button>
	<h3 id="result"></h3>
	<script>
		document.getElementById("testBtn").addEventListener("click", () => {
			let result = fetch("//localhost:9999/house/gugun")
			.then(response => response.json())
			.then((data) => {
				for(sido of data) {
					console.log(sido);
					document.getElementById("result").innerText.concat(`${sido.code} / ${sido.name}`);
				}
			});
		});
	</script>
