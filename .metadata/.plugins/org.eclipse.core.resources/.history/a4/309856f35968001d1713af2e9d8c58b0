<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.backtest.house.model.mapper.HouseMapper">
	<resultMap type="areaCode" id="areaCode">
		<result column="sidoCode" property="code"/>
		<result column="sidoName" property="name"/>
	</resultMap>
	
	<select id="getSidoCode" resultType="AreaCode">
		select left(sidoCode,2) sidoCode, sidoName
		from sidocode
		order by sidoCode
	</select>

</mapper>