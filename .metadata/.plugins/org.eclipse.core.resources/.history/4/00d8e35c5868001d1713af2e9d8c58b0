	<h3 id="result"></h3>
	<script>
		document.getElementById("testBtn").addEventListener("click", () => {
			let result = fetch("http://localhost:9999/house/sido")
			.then(response => response.json())
			.then((data) => {
				console.dir(data);
				document.getElementById("result").innerText = data;
			});
		});
	</script>
