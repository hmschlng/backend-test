<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<button id="testBtn">
		test1
	</button>
	<h3 id="result"></h3>
	<script>
		document.getElementById("testBtn").addEventListener("click", () => {
			let result = fetch("//localhost:9999/house/sido")
			.then(response => response.json())
			.then((data) => {
				console.dir(data.length);
				let result = document.getElementById("result").innerText;
				for(sido in data) {
					result = result + sido.code + "/" + sido.name;
				}
			});
		});
	</script>
</body>
</html>